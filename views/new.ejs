<% layout('layouts/boilerplate') -%>


<div class="jumbotron text-light rounded-0" style="background:#161B21">
    <div class="container">
        <h3>Add New Image</h3>
    </div>
</div>



<div class="container my-5">



<form action="/" method="POST" enctype="multipart/form-data">
    
  <div class="form-group">
    <label for="title">Title</label>
    <input type="text" id="title" class="form-control" name="post[title]" placeholder="Enter Title">
  </div>
  
  <div class="form-group">
    <label for="image">Image </label>
    <input type="file" id="image" class="form-control-file" name='image' placeholder="Upload Image">
  </div>
  
 
  <button type="submit" class="btn btn-sm btn-success my-3">Submit</button>
</form>
    
  
</div>

<div class="container">
  <a class="btn btn-sm btn-danger" href="/">Cancel</a>  
</div>


